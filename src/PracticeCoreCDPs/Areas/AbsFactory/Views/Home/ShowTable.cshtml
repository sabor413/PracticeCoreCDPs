@model System.Data.Common.DbDataReader

<html>
<head>
    <title></title>
</head>
<body>
<h1>Query Results</h1>
    
<table border="1" cellpadding="10">
    @while (Model.Read())
    {
        <tr>
            @for (int i = 0; i < Model.FieldCount; i++)
            {
                <td>@Model.GetString(i)</td>
            }
        </tr>
    }
    @{
        Model.Close();
    }
</table>
</body>
</html>
